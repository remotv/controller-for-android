<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="ffmpegDefaultFilterOptions">
        ${internal}
    </string>

    <string name="ffmpegDefaultInputOptions">
        -f rawvideo -s ${resolution} -r 25 -pix_fmt nv21 -i ${inputStream}
    </string>

    <string name="ffmpegDefaultOutputOptions">
        -f mpegts -framerate ${framerate} -codec:v mpeg1video ${bitrateflags} -bf 0 ${headers}
    </string>

    <string name="endpointHost">
        remo.tv
    </string>
</resources>